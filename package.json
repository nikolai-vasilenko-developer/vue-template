{
  "name": "aras-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test:unit": "jest --config=config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "concurrently --kill-others --raw --success first \"npm run storybook\" \"npm run test:ui\" \"deelay\"",
    "report:json": "node scripts/generate-visual-json-report.js",
    "report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "report": "npm run report:json && npm run report:html",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "serve": "webpack serve --mode development",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "lint:ts": "eslint \"src/**/*.{ts,vue,tsx}\"",
    "lint:ts:fix": "eslint \"src/**/*.{ts,vue,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
